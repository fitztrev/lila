{
    "name": "Lila Codespace",
    "image": "ghcr.io/rosen-score/lila-codespace:main",
    "extensions": [
        "scala-lang.scala"
    ],
    "forwardPorts": [
        8080,
        9663,
        9664,
        9665,
        9666
    ],
    "portsAttributes": {
        "8080": {
            "label": "Lichess dev site"
        },
        "9663": {
            "label": "lila"
        },
        "9664": {
            "label": "lila-ws"
        },
        "9665": {
            "label": "lila-fishnet (Play against computer)"
        },
        "9666": {
            "label": "lila-engine (External engine API)"
        }
    },
    "onCreateCommand": "sh /workspaces/lila/.devcontainer/scripts/onCreate.sh",
    "postCreateCommand": "sh /workspaces/lila/.devcontainer/scripts/postCreate.sh",
    "postAttachCommand": "sh /workspaces/lila/.devcontainer/scripts/postAttach.sh",
    "postStartCommand": "sh /workspaces/lila/.devcontainer/scripts/postStart.sh"
}
